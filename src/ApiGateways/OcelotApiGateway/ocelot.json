{
    "Routes": [
        {
            "DownstreamPathTemplate": "/api/{everything}",
            "DownstreamScheme": "http",
            "DownstreamHostAndPorts": [
              {
                "Host": "localhost",
                "Port": 9001
              }
            ],
            "UpstreamPathTemplate": "/api/v1/catalog/{everything}",
            "UpstreamHttpMethod": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ],
    "GlobalConfiguration": {
        "BaseUrl": "http://localhost:5000"
    }
}